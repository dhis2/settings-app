{
  "name": "settings-app",
  "version": "1.2.1",
  "description": "Settings management application for DHIS2",
  "main": "index.html",
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js --fix src `git diff --cached --name-only | grep \\.js$`",
    "validate": "npm ls --depth 0",
    "start": "webpack-dev-server",
    "prebuild": "rm -rf build",
    "build": "webpack --config webpack.config.prod.js",
    "postbuild": "cp -r index.html i18n node_modules/babel-polyfill/dist/polyfill.min.js node_modules/jquery/dist/jquery.min.js build/ && npm run manifest",
    "manifest": "d2-manifest package.json build/manifest.webapp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhis2/settings-app.git"
  },
  "keywords": [
    "DHIS2",
    "HISP",
    "Settings"
  ],
  "author": "Nicolay Ramm <nicolay@dhis2.org> (https://www.dhis2.org)",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/dhis2/settings-app/issues"
  },
  "homepage": "https://github.com/dhis2/settings-app#readme",
  "dependencies": {
    "jquery": "^2.2.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.0",
    "d2": "0.0.24",
    "d2-flux": "0.5.0",
    "d2-manifest": "^1.0.0-0",
    "d2-ui": "0.0.16-0",
    "d2-utilizr": "^0.2.3",
    "eslint": "^2.2.0",
    "eslint-config-dhis2": "^2.0.0",
    "eslint-plugin-react": "^4.0.0",
    "fbjs": "^0.7.2",
    "lodash.isfinite": "^3.3.0",
    "loglevel": "1.4.0",
    "material-ui": "^0.14.4",
    "moment": "^2.11.2",
    "node-sass": "^3.4.2",
    "precommit-hook": "^3.0.0",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-tap-event-plugin": "0.2.2",
    "rx": "^3.1.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "susy": "^2.2.6",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "pre-commit": [
    "validate"
  ],
  "webapp.manifest": {
    "name": "DHIS 2 System Settings Management",
    "icons": {
      "48": "dhis-web-settings.png"
    },
    "activities": {
      "dhis": {
        "href": ".."
      }
    }
  }
}
