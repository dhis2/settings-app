{
  "name": "settings-app",
  "version": "31.0.0",
  "description": "Settings management application for DHIS2",
  "main": "index.html",
  "scripts": {
    "build": "webpack --mode production",
    "lint": "eslint .",
    "manifest": "d2-manifest package.json build/manifest.webapp",
    "postbuild": "cp -r ./package.json *.png i18n build/ && npm run manifest",
    "prebuild": "rm -rf build",
    "start:manifest": "d2-manifest package.json manifest.webapp",
    "start:dev": "webpack-dev-server --mode development",
    "start": "npm run start:manifest && npm run start:dev",
    "validate": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhis2/settings-app.git"
  },
  "keywords": [
    "DHIS2",
    "HISP",
    "Settings"
  ],
  "greenkeeper": {
    "ignore": [
      "d2"
    ]
  },
  "author": "Nicolay Ramm <nicolay@dhis2.org> (https://www.dhis2.org)",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/dhis2/settings-app/issues"
  },
  "homepage": "https://github.com/dhis2/settings-app#readme",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@dhis2/d2-ui-header-bar": "^5.2.6",
    "classnames": "^2.2.5",
    "d2": "^30.0.0",
    "d2-manifest": "^1.0.0",
    "d2-ui": "^29.0.5",
    "d2-utilizr": "^0.2.15",
    "d3-color": "^1.0.3",
    "fixed-data-table": "^0.6.4",
    "history": "^4.7.2",
    "loglevel": "^1.4.1",
    "material-ui": "^0.20.0",
    "moment": "^2.18.1",
    "prop-types": "^15.6.2",
    "react": "16.8.3",
    "react-dom": "^16.7.0",
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "7.3.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "eslint-config-dhis2": "^3.0.3",
    "html-webpack-plugin": "^3.2.0",
    "node-pre-gyp": "^0.12.0",
    "node-sass": "4.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "susy": "^3.0.5",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "manifest.webapp": {
    "name": "DHIS 2 System Settings Management",
    "icons": {
      "48": "icon.png"
    },
    "dhis2": {
      "apiVersion": "30"
    },
    "activities": {
      "dhis": {
        "href": ".."
      }
    }
  }
}
