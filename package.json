{
  "name": "settings-app",
  "version": "25.0.0",
  "description": "Settings management application for DHIS2",
  "main": "index.html",
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js --fix src `git diff --cached --name-only | grep \\.js$`",
    "validate": "npm ls --depth 0",
    "start": "d2-manifest package.json manifest.webapp && webpack-dev-server",
    "prebuild": "rm -rf build",
    "build": "webpack",
    "postbuild": "cp -r index.html *.png i18n node_modules/jquery/dist/jquery.min.js build/ && npm run manifest",
    "manifest": "d2-manifest package.json build/manifest.webapp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhis2/settings-app.git"
  },
  "keywords": [
    "DHIS2",
    "HISP",
    "Settings"
  ],
  "author": "Nicolay Ramm <nicolay@dhis2.org> (https://www.dhis2.org)",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/dhis2/settings-app/issues"
  },
  "homepage": "https://github.com/dhis2/settings-app#readme",
  "dependencies": {
    "jquery": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.6.4",
    "babel-eslint": "^6.0.0-beta.1",
    "babel-loader": "^6.2.3",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.0",
    "d2": "^24.0.1",
    "d2-manifest": "^1.0.0",
    "d2-ui": "^24.0.3",
    "d2-utilizr": "^0.2.4",
    "eslint": "^2.2.0",
    "eslint-config-dhis2": "^2.0.0",
    "eslint-plugin-react": "^4.0.0",
    "fbjs": "^0.7.2",
    "http-proxy": "git+https://github.com/nicolayr/node-http-proxy.git",
    "lodash.isfinite": "^3.3.0",
    "loglevel": "1.4.0",
    "material-ui": "^0.14.4",
    "moment": "^2.11.2",
    "node-sass": "^3.4.2",
    "precommit-hook": "^3.0.0",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-tap-event-plugin": "0.2.2",
    "rx": "^4.0.8",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "susy": "^2.2.6",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "fixed-data-table": "~0.6.0"
  },
  "pre-commit": [
    "validate"
  ],
  "manifest.webapp": {
    "name": "DHIS 2 System Settings Management",
    "icons": {
      "48": "icon.png"
    },
    "activities": {
      "dhis": {
        "href": ".."
      }
    }
  }
}
